// On Page load
function showDefaultToolTip() {
  const suburbButton = document.querySelector('[data-testid="suburb-button"]');
  if(suburbButton !== null){
    const postcodeDiv = suburbButton.querySelector('.postcode-text');
    const postcodeHTML = postcodeDiv.innerHTML;

    const postcodeindex = postcodeHTML.lastIndexOf(' ');
    const postcode = postcodeHTML.substring(postcodeindex + 1);

    toolTipConfig(postcode);
  }
}

function toolTipConfig(postcode){
    const postcodebanner = document.querySelector('[data-testid="post-code-banner"]');
    postcodebanner.style.position = "relative"; // Add relative css to the parent element.

    //const postcodeArray = [3172, 3173, 3174, 3175, 3021, 3036, 3037, 3038, 3042, 3045, 3027, 3029];
   const postcodeArray = [3026, 3027, 3028, 3029, 3030, 3033, 3036, 3037, 3038, 3041, 3042, 3043, 3045, 3046, 3047, 3048, 3049, 3059, 3061, 3063, 3171, 3172, 3173, 3174, 3175];

    // Get the div element with class "tooltiptext"
    const tooltipDiv = document.querySelector(".tooltiptext");
    
     // Remove the div element
     if(tooltipDiv !== null) {
       //alert("Not empty");
         tooltipDiv.remove();
     }

     if (postcodeArray.includes(parseInt(postcode))) {
                
        const postCodeDiv = document.querySelector("[data-testid='suburb-button']");

      // Tooltip code goes here
      const tooltipBlock = `
      <span class="tooltiptext"><button class="tooltip-close">&times;</button><b>Direct to Boot</b> is available in your area. Try it now and earn <b>1000 bonus Flybuys points.</b> <br> <a href="/directtoboot" class="learn-more-dtb"> Learn More</a></span>
          
          <style>
          .tooltiptext {
              display: block;
              width: 240px;
              background-color: #0069B6;
              color: #fff;
              text-align: center;
              padding: 16px 32px 16px 16px;
              position: absolute;
              z-index: 998;
              top: 120%;
              left: 47.5%;
              margin-left: -60px;
              line-height: 1.4;
              text-align:left;
              font-size:.75rem;
              line-height:1.125rem;
              letter-spacing:.4px;
							
          }
					.popup-notification{
					z-index:999 !important;
}
        
          @media only screen and (max-width:470px){
             .tooltiptext {
                left: 35% !important;
            }
          }
          .tooltiptext a{
              color:#ffffff !important;
              text-decoration: underline !important;
          }

          .tooltiptext::after {
              content: "";
              position: absolute;
              bottom: 100%;
              left: 80%;
              margin-left: -5px;
              border-width: 5px;
              border-style: solid;
              border-color: transparent transparent #0069B6 transparent;
          }

              
          
@media only screen and (max-width:470px){
              .tooltiptext::after {
                  left: 80%;
            }
          }

          .tooltip-close {
              position: absolute;
              top: 7px;
              right: 5px;
              padding: 5px;
              color: #fff;
              font-size: 1.5rem;
              line-height: 1.875rem;
              background-color: transparent;
              border: none;
              cursor: pointer;
          }
          
          </style>
      `;
      
      postCodeDiv.insertAdjacentHTML("afterend", tooltipBlock);
      const tooltipClose = document.querySelector(".tooltip-close");

      tooltipClose.addEventListener('click', function() {
          const tooltipDiv = document.querySelector(".tooltiptext");
          tooltipDiv.remove();
      });

      setTimeout(function(){
        const tooltipDiv = document.querySelector(".tooltiptext");
                         tooltipDiv.remove();
     }, 30000);

      // Save the timestamp when the tooltip was shown
       sessionStorage.setItem('tooltipShown', Date.now());
      
  }

}


