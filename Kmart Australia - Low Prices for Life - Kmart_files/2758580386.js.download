								'price': item.getAttribute('data-price') ? item.getAttribute('data-price').trim().replace('$', '') : '0',
								'dimension19': item.getAttribute('data-style'),
								'dimension29': postcode,
								'position': item.getAttribute('data-pos')
							}]
						}
					},
					'assigned_postcode': postcode
				});
				window.location.href = item.href;		
		});
	});
	dataLayer.push({
		'event': 'product_list_impression',
		'list': LIST_NAME,
