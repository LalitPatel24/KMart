function getOOS() {
	extension.oosSelectors = [];
	extension.targetId = []

	var oosEles = document.querySelectorAll('.MuiAlert-message');
	for (var i = 0; i < oosEles.length; i++) {
		if (oosEles[i].innerText.length > 0) {
			if (oosEles[i].textContent == 'Out of stock online' || oosEles[i].innerText.textContent == 'Pickup at another store') {
				var targetID = oosEles[i].parentElement.parentElement.parentElement.firstChild.firstChild.href.split("?")[0].split("-").slice(-1)[0];
				extension.oosSelectors.push(oosEles[i]);
				extension.targetId.push(targetID);
			}
		}
	}
	if (extension.oosSelectors.length > 0) {
		checkSelectors(extension.oosSelectors, extension.targetId)
	} else {
		if (disconnected == true) {
			checkPostcode();
		}
	}
}

let checkSelectors = (oosProducts, targetID) => {
	if (oosProducts.length && targetID.length) {
		if (oosProducts.length > 0) {
			utils.Promise.resolve(getTargetId(targetID[0]))
				.then(fetchRecos)
				.then(prepareHTML)
				.then(function() {
					if (disconnected == true) {
						checkPostcode();
					}
				})
		}
	}
}



let checkPostcode = () => {
	storeClick();
	disconnected = false;
	utils.waitForElement("div[data-testid='postcode-autosuggest'] > div > div > div")
		.then(function() {
			var targetNode = document.querySelector("[data-testid='postcode-autosuggest']");
			var options = {
				childList: true,
				attributes: true,
				subtree: true
			};
			var callback = function(mutationsList) {
				for (var mutation of mutationsList) {
					if (mutation.type == 'childList') {
						postcodeSubmit();
					}

				}
			}

			function postcodeSubmit() {
				listener = 1;
				let cnc = document.querySelector("#tab-1").ariaSelected
				let time;
				if (cnc == 'true') {
					time = 5000;
				} else {
					time = 1500;
				}
				var postcodeBox = document.querySelector("ul.MuiAutocomplete-listbox");
				if (postcodeBox) {
					postcodeBox.addEventListener("click", function() {
						let recos = document.querySelectorAll(".oosRecoContainer");
						if (recos.length > 0) {
							recos.forEach(function(recs) {
								recs.remove();
							})
						}

						observerPostcode.disconnect();
						disconnected = true;
						utils.waitForElement("[data-testid*='ModalNotification']").then(function(element) {
							setTimeout(getOOS, time);
						})
					});
				}
			};
			var observerPostcode = new MutationObserver(callback);
			observerPostcode.observe(targetNode, options);

			let cnc = document.querySelector("button#tab-1")
			cnc.addEventListener("click", function() {
				observerPostcode.disconnect();
			})
			let del = document.querySelector("button#tab-0")
			del.addEventListener("click", function() {
				listener = 1;
				observerPostcode.disconnect();
				disconnected = true;
			})
		})
}

let storeClick = () => {
	let cnc = document.querySelector("#tab-1").ariaSelected
	if (cnc == "true" && listener == 1) {
		listener++;
		utils.waitForElement("[name='store']").then(function(element) {
			let storeList = document.querySelectorAll("[name='store']")
			storeList.forEach(store => {
				store.addEventListener("click", function() {
					let recos = document.querySelectorAll(".oosRecoContainer");
					if (recos.length > 0) {
						recos.forEach(function(recs) {
							recs.remove();
						})
					}
					setTimeout(getOOS, 1500);
				})

			})

		})
	}
}
