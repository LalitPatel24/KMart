//Require
var utils = optimizely.get('utils');
var recommender = optimizely.get('recommender');


/*---Enable debugging params--*/
var logEnabled = false || window.localStorage.getItem('opti_logRecos');
window.logEnabled = logEnabled;
var log = function() {
	if (logEnabled) {
		var args = Array.prototype.slice.call(arguments);
		args.unshift("OPTI:");
		console.log.apply(console, args);
	}
};
extension.loadExample = false || window.localStorage.getItem('opti_sampleRecos');
/*---Enable debugging params--*/


/*--- EXTENSION PARAMS AND GLOBAL VARS ---*/
var extension = extension || {};
extension.max = 4;
window.recommendations = window.recommendations || {};
window.recommendations.useReco;
window.recommendations.key;
window.recommendations.algorithm = extension.recoAlgorithm;
var counter = 6;
var foundProducts = []


function checkRecos() {
	log("checking recos", recommender);
	log(window.recommendations)
	if (!window.recommendations.gatewayRecos && counter > 0) {
		log("COUNTER:", counter);
		counter--;
		setTimeout(checkRecos, 500);
	} else if (extension.recoAlgorithm === 'optimizely') {
		log("using opti recos")
		window.recommendations.useReco = window.recommendations.optiRecos;
		window.recommendations.key = 'product_keycode';
		getItemData(0, window.recommendations.useReco)
	} else if (extension.recoAlgorithm === 'apigateway') {
		log("using api gatewayRecos")
		window.recommendations.useReco = window.recommendations.gatewayRecos;
		window.recommendations.key = 'recommended_product_id';
		getItemData(0, window.recommendations.useReco)
	}
}



var recommendedItems;

/*--- EXTENSION PARAMS AND GLOBAL VARS ---*/


var storeRecoData = (index, data, recommendedItems) => {
	log("STORE RECO DATA", index, data);
    
	try {
		if (data.sections.Products.length != 0) {
			let foundProduct = data.sections.Products[0]
			let slicedID = ((foundProduct.data.id)).slice(2)
			let foundImage = (foundProduct.data.image_url).match(/\/undefined/)
			if (slicedID[0] == 'S' && (foundImage == null)) {
				
					let foundID = (foundProduct.data.id).match((/S\d+/))[0]
					foundProducts.push({
						'product_name': foundProduct.value,
						'product_price': foundProduct.data.price,
						'product_sku': foundID,
						'product_url': `${window.location.origin}${foundProduct.data.uri}`,
						'product_image_link': foundProduct.data.image_url,
						'variation_id': foundProduct.data.variation_id,
					})

			} else {
				
					let foundID = (foundProduct.data.id).match((/\d+/))[0]
					log("foundID regex", foundID)
					if (foundImage == null) {
						foundProducts.push({
							'product_name': foundProduct.value,
							'product_price': foundProduct.data.price,
							'product_sku': foundID,
							'product_url': `${window.location.origin}${foundProduct.data.uri}`,
							'product_image_link': foundProduct.data.image_url,
							'variation_id': foundProduct.data.variation_id,
						})
                        
					}
				
			}
		}
	} catch (err) {
		log("Error fetching product details", err)
	}
	index++;

	if (foundProducts.length < extension.max) {
		log("GET NEXT ITEM");
		getItemData(index, recommendedItems);
	} else {
		renderRecos(foundProducts);
	}
};

var getItemData = (currentIndex, recommendedItems) => {
	//recommendedItems should be array of objects
	log("Get item data");
	let sku = recommendedItems[currentIndex][window.recommendations.key];
    let constructorApi = `https://ac.cnstrc.com/autocomplete/P_${sku}?autocomplete_key=key_GZTqlLr41FS2p7AY`
	fetch(constructorApi, {
			method: "GET",
			headers: {
				'Content-Type': 'application/json',
			},

		})
		.then(response => response.json())
		.then(recommended_item => storeRecoData(currentIndex, recommended_item, recommendedItems))
};

function postFilter(recos) {

    try{
        // Truncate long product names with an ellipsis  
        function text_truncate(str) { //passed value from the catalogue can be null 
            let newStr = str || ""; //coercing null and undefined values to empty string 
            newStr = (newStr.length > 48) ? newStr.substr(0, 48) + "" : newStr;
            return newStr;
        }

        truncatedRecos = []
        for (item in recos) {
            let productName = recos[item].product_name || "";
            // Replace &amp; with & in Product name
            if (productName.indexOf("&amp;") !== -1) {
                productName = productName.replace("&amp;", "&");
            }
            recos[item].product_name = text_truncate(productName);
			log("recos[item].product_price = (recos[item].product_price)", recos[item].product_price = (recos[item].product_price))
            recos[item].product_price = (recos[item].product_price).toFixed(2);
            truncatedRecos.push(recos[item]);
        }

        return truncatedRecos;
    }
    catch(err){
        log(err)
    }	
}


function renderRecos(recos) {
	log("RENDER RECOS.");
	recos = recos.slice(0, extension.max);
	recos = postFilter(recos)
	if (recos.length === 0) {

		dataLayer.push({
			'event': 'myTrackEvent',
			'eventCategory': 'Optimizely',
			'eventAction': 'Personalisation',
			'eventLabel': 'zero products available'
		});
		return;
	}
	recos.forEach(function(r, index) {
		recos[index].product_num = index + 1;
	});
	log("OPTI: Generate HTML");
	try {
		var html = extension.$render({
			extension: extension,
			recos: recos,
		});
	} catch (e) {
		log("OPTI: ERROR:", e);
	}
	log("OPTI: HTML IS:", html);
	if (recos.length) {
		$(document.body).addClass("optly-hidden-minicart");
		utils.waitForElement("#mini-bag-trigger").then(function() {
			document.getElementById("mini-bag-trigger").setAttribute("style", "display:none");
			$(document.body).addClass("optly-hidden-minicart");
			$("body").append(html).addClass("optly-popup-active");
			window['optimizely'].push({
				type: "event",
				eventName: "minicart_recos_displayed"
			});
			document.querySelector("button.optly-view-bag-btn").addEventListener("click", function() {
				document.location = "https://www.kmart.com.au/checkout/bag";
			});
			document.querySelector("#optly-minicart-btn-bag a").addEventListener("click", function() {
				document.querySelector(".optly-modal-container").remove()
			});
      document.querySelector("[class='close-modal-dialog square right optly-icon-close']").addEventListener("click", function() {
				document.querySelector(".optly-modal-container").remove()
			});
		});

	}
}



let waitAdd2Cart = function() {
	log("waiting for cart")
	if (recommender) {
		utils.waitForElement("#optly-modal-recs-wrapper .recommended_item a").then(function(elem) {
			dataLayer.push({
				"event": "recommenderImpressions",
				"elem": $("#optly-modal-recs-wrapper")
			});
			$("#optly-modal-recs-wrapper .recommended_item a").unbind('click').bind('click', function(e) {
				dataLayer.push({
					"event": "recommenderClick",
					"elem": $(this)
				});

			});
			impressionData = [];
			document.querySelectorAll(".optly-modal-reco-container .recommended_item a").forEach(function(item) {
				impressionData.push({
					'id': item.dataset.name,
					'name': 'PDP Add to Bag Also Bought',
					'creative': 'PDP Add to Bag',
					'position': item.dataset.pos
				});
				item.addEventListener('click', e => {
					dataLayer.push({
						'event': 'promotionClick',
						'ecommerce': {
							'promoClick': {
								'promotions': [{
									'id': item.dataset.name,
									'name': 'PDP Add to Bag Also Bought',
									'creative': 'PDP Add to Bag',
									'position': item.dataset.pos
								}]
							}
						}
					});
				});
			});
			dataLayer.push({
				'event': 'internal_promotion',
				'ecommerce': {
					'promoView': {
						'promotions': impressionData
					}
				}
			});
		});
		log("OPTI: recommender");
		document.querySelector("[data-testid='product-button']").addEventListener("click", function(e) {
			foundProducts = []
			log("OPTI: ADD TO BAG");

			extension.productName = document.querySelector("[data-testid='product-button']").innerText;
			if (extension.productName.length > 66) {
				extension.productName = extension.productName.substr(0, 66);
				extension.productName += "";
			}

			checkRecos();

		});
	}
}

log("starting recos add2cart")
utils.waitForElement("[data-testid='wishlist-button-container']").then(waitAdd2Cart)




