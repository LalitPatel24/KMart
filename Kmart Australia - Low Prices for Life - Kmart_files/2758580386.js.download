/* Optimizely library */
let utils = optimizely.get('utils');

const DISPLAY_RECOS_NUMBER = 3; // how many products recommended
const CAMPAIGN_NAME = 'PERS_202308_PDP-GM-SUBSTITUTE-PRODUCT_REC_WEB_TRIG_PROD_VAR2_33'; // name of the campaign, including variant
const LIST_NAME = 'PDP Substitute Products Var2'; // Google Analytics list name

/* START: Enable debugging params */
let logEnabled = false || window.localStorage.getItem('opti_log');
let log = function() {
	if (logEnabled) {
		var args = Array.prototype.slice.call(arguments);
		args.unshift("OPTI:", CAMPAIGN_NAME);
		console.log.apply(console, args);
	}
};
/* END: Enable debugging params */

/* local host, pathname and recommender API */
let host = window.location.hostname.replace(/^www\./, '');
var uri = document.location.pathname; // used for error tracking
let recosAPI = 'https://api.' + host + '/personalisation/product/subs?product_id=';

/* find DOM element that has product SKU; get product SKU value; delete 'P_' */
let product_sku = document.querySelector('div.product-description-sku > p > span').innerText.slice(2);

var fetchRecos = (api, sku) => {
	fetch(
      `${api}${sku}`, {
				method: "GET",
				headers: {
					'Content-Type': 'application/x-www-form-urlencoded; charset=UTF-8',
				},
      }
    )
		.then(res => res.json())
		.then(result => {
			let recosSkus = [];
//log("RECOS COUNT:", result.recommended_products.length);
			for (let p in result.recommended_products) {
				recosSkus.push(result.recommended_products[p].recommended_product_id);
			}
//log("RECOS SKUS:", recosSkus);
			getProducts(recosSkus, DISPLAY_RECOS_NUMBER);

		})
		.catch(error => ({
			'error': error
		}));
};

let getProducts = (productArr, maximumCount) => {
	let productRecos = [];
	let productSearchArr = [];
	for (let productID in productArr) {
		productSearchArr.push(productSearch(productArr[productID]));
	}
	try {
		productRecos = Promise.all(productSearchArr).then(productDetails => {
			let foundProducts = [];
			for (let product in productDetails) {
				if (productDetails[product].sections.Products.length != 0) {
					let foundProduct = productDetails[product].sections.Products[0];
					let foundID = foundProduct.data.id;
					let matched = productArr.find(str => `p_${str}` === foundID.toLowerCase());
					if(matched) {
						foundProducts.push({
							'product_name': foundProduct.value,
							'product_price': foundProduct.data.price,
							'product_id': foundID.replace('P_', ''),
							'product_url': `${window.location.origin}${foundProduct.data.uri}`,
							'product_image_link': foundProduct.data.image_url,
							'style_id': foundProduct.data.variation_id,
							'position': foundProducts.length + 1,
						});
					}
				}
			}
//log("ALL PRODUCTS:", foundProducts);
			return foundProducts.slice(0, maximumCount);
		}).then(data => {
//log("RECOMMENDERS:", data);
		if (data.length >= DISPLAY_RECOS_NUMBER) {
			renderRecos(data);
		} else {
			sendErrorToNewRelic(
				new Error(`Number of recos is not meeting the requirement, substitute recommendations are not displayed`),
				errorLevel='Warning'
			);
			return;
		}			
		});
	} catch (error) {
		log(error);
	}
};

let productSearch = (productID) => {
	return fetch(
			`https://ac.cnstrc.com/autocomplete/P_${productID}?autocomplete_key=key_GZTqlLr41FS2p7AY`, {
				method: "GET",
				headers: {
					'Content-Type': 'application/json',
				},
			}
		)
		.then((res) => res.json())
		.then((result) => result)
		.catch(error => ({
			'error': error
		}));
}

/**
 * Rendering the list of product details on the UI
 */
function renderRecos(recos) {
  	let placementLocation = "[data-testid='wishlist-button']";
 		recos = postFilter(recos);
		var html = extension.$render({
		extension: extension,
		recos: recos,
	});
	let holder = document.querySelector(placementLocation);
//log("PLACEHOLDER:", placementLocation);
	holder.insertAdjacentHTML('afterend', html);
	applyGATracking();
}

/**
 * This function checks the validity of the product detail and corrects the string of the PD
 * @param {*} reco the product details 
 * @returns 
 */
function postFilter(recos) {
	// Truncate long product names with an ellipsis  
	function text_truncate(str) { //passed value from the catalogue can be null 
		let newStr = str || ""; //coercing null and undefined values to empty string 
		newStr = (newStr.length > 100) ? newStr.substr(0, 100) + "" : newStr;
		return newStr;
	}

	truncatedRecos = [];
	for (var item in recos) {
		let productName = recos[item].product_name || "";
		// Replace &amp; with & in Product name
		if (productName.indexOf("&amp;") !== -1) {
			productName = productName.replace("&amp;", "&");
		}
		recos[item].product_name = text_truncate(productName);
		recos[item].product_price = (recos[item].product_price).toFixed(2);
		truncatedRecos.push(recos[item]);
	}

	return truncatedRecos;
}

/* get post code from UI element */
let getPostCode = () => {
	let postCodeRegex = /\d{4}/;
	let postCode = '2000';
	$('.postcode-text.ellipsis').each(function() {
		let match = $(this).text().match(postCodeRegex);
		if (match) {
			postCode = match[0];
		}
	});
	return postCode;
};

