var counter = 6;

function checkRecos() {
	try{
		if (!window.recommendations.gatewayRecos && counter > 0) {
			log("COUNTER:", counter);
			counter--;
			setTimeout(checkRecos, 500);
		} else if (extension.recoAlgorithm === 'optimizely') {
			try{
			window.recommendations.useReco = window.recommendations.optiRecos;
			window.recommendations.key = 'product_keycode';
			getItemData(0, window.recommendations.useReco)}
			catch(err){
                sendErrorToNewRelic(new Error("optiRecos not available"))
				
			}
				
		} else if (extension.recoAlgorithm === 'apigateway') {
			try{
				window.recommendations.useReco = window.recommendations.gatewayRecos;
				window.recommendations.key = 'recommended_product_id';
				getItemData(0, window.recommendations.useReco)
			}
			catch(err){
				sendErrorToNewRelic(new Error("gateway not available"))
			}	
		}
	} catch(err){
        sendErrorToNewRelic(err)
	}
}

var foundProducts = [];
var compareID = []
var recommendedItems;
//contains product information in optimizely
var catalogLocation;
/*--- EXTENSION PARAMS AND GLOBAL VARS ---*/

var renderRecos = (recos) => {
    try{
        recos = recos.slice(0, extension.max);
        recos = postFilter(recos)
        if (recos.length === 0) {
            return;
        }
        recos.forEach(function(r, index) {
            recos[index].product_num = index + 1;
        });
        log("Generate HTML");
        var html = extension.$render({
            extension: extension,
            recos: recos,
        });
        log("HTML IS:", html);
        utils.waitForElement('main')
            .then(function(elem) {
                // Prepend the extension html to the body
                elem.insertAdjacentHTML('afterend', html);
                log("Hello");
            });
    }
    catch(err){
        sendErrorToNewRelic(err)
    }
	
}

function postFilter(recos) {

    try{
        // Truncate long product names with an ellipsis  
        function text_truncate(str) { //passed value from the catalogue can be null 
            let newStr = str || ""; //coercing null and undefined values to empty string 
            newStr = (newStr.length > 48) ? newStr.substr(0, 48) + "" : newStr;
            return newStr;
        }

        truncatedRecos = []
        for (item in recos) {
            let productName = recos[item].product_name || "";
            // Replace &amp; with & in Product name
            if (productName.indexOf("&amp;") !== -1) {
                productName = productName.replace("&amp;", "&");
            }
            recos[item].product_name = text_truncate(productName);
            recos[item].product_price = (recos[item].product_price).toFixed(2);
            truncatedRecos.push(recos[item]);
        }

        return truncatedRecos;
    }
    catch(err){
        sendErrorToNewRelic(err);
    }	
}


var storeRecoData = (index, data, recommendedItems) => {
	log("STORE RECO DATA", index, data);
    
	try {
		if (data.sections.Products.length != 0) {
			let foundProduct = data.sections.Products[0]
			let slicedID = ((foundProduct.data.id)).slice(2)
			let foundImage = (foundProduct.data.image_url).match(/\/undefined/)
			if (slicedID[0] == 'S' && (foundImage == null)) {
				if (compareID.indexOf(foundProduct.data.id) == -1) {
					let foundID = (foundProduct.data.id).match((/S\d+/))[0]
					foundProducts.push({
						'product_name': foundProduct.value,
						'product_price': foundProduct.data.price,
						'product_sku': foundID,
						'product_url': `${window.location.origin}${foundProduct.data.uri}`,
						'product_image_link': foundProduct.data.image_url,
						'variation_id': foundProduct.data.variation_id,
					})
                    compareID.push(foundProduct.data.id)
				}

			} else {
				if (compareID.indexOf(foundProduct.data.id) == -1) {
					let foundID = (foundProduct.data.id).match((/\d+/))[0]
					log("foundID regex", foundID)
					if (foundImage == null) {
						foundProducts.push({
							'product_name': foundProduct.value,
							'product_price': foundProduct.data.price,
							'product_sku': foundID,
							'product_url': `${window.location.origin}${foundProduct.data.uri}`,
							'product_image_link': foundProduct.data.image_url,
							'variation_id': foundProduct.data.variation_id,
						})
                        compareID.push(foundProduct.data.id)
					}
				}

			}
		}
	} catch (err) {
        sendErrorToNewRelic(err)
		log("Error fetching product details", err)
	}
	index++;

	if (foundProducts.length < extension.max) {
		log("GET NEXT ITEM");
		getItemData(index, recommendedItems);
	} else {
		renderRecos(foundProducts);
	}
};

var getItemData = (currentIndex, recommendedItems) => {
	//recommendedItems should be array of objects
	log("Get item data");
	let sku = recommendedItems[currentIndex][window.recommendations.key];
    let constructorApi = `https://ac.cnstrc.com/autocomplete/P_${sku}?autocomplete_key=key_GZTqlLr41FS2p7AY`
	fetch(constructorApi, {
			method: "GET",
			headers: {
				'Content-Type': 'application/json',
			},

		})
		.then(response => response.json())
		.then(recommended_item => storeRecoData(currentIndex, recommended_item, recommendedItems))
		.catch(error => sendErrorToNewRelic(new Error(`Failed to fetch constructor API ${constructorApi}`)));
};

