var checkImage = (reco) => {
	var img = reco.produt_image || "";
	let sku = reco.product_keycode
	// Replace &amp; with & in Product name
	if (!img || img.indexOf("noimageloader") !== -1) {
		return fetch(
				`https://ac.cnstrc.com/autocomplete/P_${sku}?autocomplete_key=key_GZTqlLr41FS2p7AY`, {
					method: "GET",
					headers: {
						'Content-Type': 'application/json',
					},
				}
			)
			.then(response => response.json())
			.then(data => {
				try {
					if (data.sections.Products.length != 0) {
						let foundProduct = data.sections.Products[0]
						let slicedID = ((foundProduct.data.id)).slice(2)
						let foundImage = (foundProduct.data.image_url).match(/\/undefined/)
						if (slicedID[0] == 'S' && (foundImage == null)) {
							reco.product_image = foundProduct.data.image_url
						} else {
							if (foundImage == null) {
								reco.product_image = foundProduct.data.image_url
							}
						}
					}
					return (reco)

				} catch (err) {
					log(`OPTI ${experimentName}: extension not activated, error fetching product image detail`, err);
					sendErrorToNewRelic(
					new Error( `${experimentName}: Error while fetching product image`),
						errorLevel = 'Warning'
					)
					return false;
				}
			})
			.catch(function(error) {
				sendErrorToNewRelic(
					new Error(`${experimentName}: Error while fetching constructors API`),
					errorLevel = 'Warning'
				)
			});
	}
}

