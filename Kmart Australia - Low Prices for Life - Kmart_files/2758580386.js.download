var logEnabled = false || window.localStorage.getItem('opti_logRecos');
window.logEnabled = logEnabled;
let log = function() {
	if (logEnabled) {
		var args = Array.prototype.slice.call(arguments);
		args.unshift("OPTI:");
		console.log.apply(console, args);
	}
};

var campaignName = "PERS_20230216_OOS-CART-RECOS_REC_WEB_TRIG_PROD"

function sendErrorToNewRelic(
	error = new Error('Uncaught Error'),
	errorLevel = 'Critical'
) {
	log('sendErrorToNewRelic: ', error)
	newrelic.noticeError(
		error, {
			'message': `PERSO Optimizely ${errorLevel} Error`,
			'Request Uri': location.pathname,
			'op-exp-name': campaignName,
		}
	);
};

extension.loadExample = false || window.localStorage.getItem('opti_sampleRecos');
extension.recoID = 0;


var recommenderIds = {
	"co-buy": 0,
	"co-browse": 21325510588,
	"popular": 0,
	"co-filter": 21337850740,
	"popular-appliances": 0,
	"popular-womens-clothing": 0
};

var recommenderKey = {
	recommenderServiceId: 21329930299, //catalogue id
	recommenderId: recommenderIds[extension.algorithm] //recommender id
};

var getTargetId = function(targetID) {
	// * For most-popular algorithms, this should be a fixed value of 'popular'.
	// * For user-based algorithms, this will be the optimizely visitor id which you can get through this code:
	//   optimizely.get('visitor').visitorId
	// * For item-based algorithms such as co-browse and co-buy, this will be the target item id.
	try {
		var algorithm = 'popular';
		switch (extension.algorithm) {
			case 'popular':
				algorithm = 'popular';
				break;
			case 'co-filter':
				algorithm = optimizely.get('visitor').visitorId;
				break;
			case 'co-browse':
			case 'co-buy':
				var sku = targetID;
				algorithm = sku;
				break;
			default:
				algorithm = 'co-browse';
				break;
		}
		return algorithm;
	} catch (e) {
		sendErrorToNewRelic(
			new Error(`${experimentName}: Failed to fetch Optimizely recommendation algorigthm, extension is not activated`)
		)
		log(`OPTI ${experimentName}: target ID error:`, e);
	}
};




var canonicalize = function(recommendation) {
	var reco = recommendation;

	// In the example below, we convert numeric prices into string form, and treat missing in_stock values as true.
	if (typeof reco.product_price === 'number') {
		// [MUST CHANGE] if this is for a different currency and/or locale.
		var symbol = '$';
		var locale = 'en-AU';
		reco.product_price = symbol + (reco.product_price / 100.0).toLocaleString(
			locale, {
				minimumFractionDigits: 2,
				maximumFractionDigits: 2
			});
	}
	if (typeof reco.in_stock === 'undefined') {
		reco.in_stock = true;
	}
	return reco;
};


var postFilter = function(reco) {

	var product_name = reco.product_name || "";
	if (product_name.indexOf("&") >= 0) {
		reco.product_name = reco.product_name.replace("&amp;", "");
	}
	// Truncate long product names with an ellipsis  
	function text_truncate(str) { //passed value from the catalogue can be null 
		var str = str || ""; //coercing null and undefined values to empty string 
		str = (str.length > 27) ? str.substr(0, 27) + "..." : str;
		return str;
	}
	reco.product_name = text_truncate(product_name);

	return reco;
};

var fetchRecos = function(targetId) {
	try {
		var fetcher = recommender.getRecommendationsFetcher(recommenderKey, targetId, {
			canonicalize: canonicalize,
			postFilter: postFilter
		});
		return fetcher.next(extension.max);
	} catch (error) {
		sendErrorToNewRelic(
			new Error(`${experimentName} Error while fetching recommendations, extension is not activated`, error)
		)
	}
};


